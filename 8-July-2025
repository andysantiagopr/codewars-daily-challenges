function grow(x) {
  return x.reduce((accumulator, currentValue) => accumulator * currentValue, 1);
  // Uses the reduce method to multiply all elements in the array sequentially, starting with an initial value of 1.
}

// âœ… Example Usage
console.log(grow([1, 2, 3, 4])); // âžž 24
console.log(grow([1, 5]));       // âžž 5
console.log(grow([2, 2, 2]));    // âžž 8
