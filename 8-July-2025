function paperwork(n, m) {
  return (n < 0 || m < 0) ? 0 : n * m; 
  // Returns 0 if either n or m is negative; otherwise, returns the product of n and m using a ternary operator.
}

// âœ… Example Usage
console.log(paperwork(5, 5));   // âžž 25
console.log(paperwork(5, -5));  // âžž 0
console.log(paperwork(-5, -5)); // âžž 0


