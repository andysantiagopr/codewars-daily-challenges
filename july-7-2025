function openOrSenior(data){
  return data.map(person => {
    if (person[0] >= 55 && person[1] > 7) {
      return 'Senior';
    } else {
      return 'Open';
    }
  });
}

// âœ… Example Usage
console.log(openOrSenior([[18, 20], [45, 2], [61, 12], [78, 9]])); 
// âžž ["Open", "Open", "Senior", "Senior"]
